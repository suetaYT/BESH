{
  "name": "vtornik2",
  "version": "1.0.0",
  "description": "Full-stack application with React frontend and Django backend",
  "scripts": {
    "start-frontend": "cd frontend && npm start",
    "build-frontend": "cd frontend && npm run build",
    "start-backend": "cd backend && python manage.py runserver",
    "start-dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
} 